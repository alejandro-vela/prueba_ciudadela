{
    "name": "ciudadela",
    "version": "1.0.0",
    "description": "undefined",
    "scripts": {
        "dev": "nodemon index.ts",
        "start:local": "npm run build:ts && export ENV=local && cp -r environments/. dist/environments && node dist/index.js",
        "copy:env:local": "mkdir -p ./dist/environments && cpy environments/.local.env dist/. && cpy --rename docker-compose.yml docker-compose.local.yml ./dist/.",
        "build:local": "npm run build:ts && npm run copy:env:local && npm run copy:config",
        "copy:config": "cpy package.json Dockerfile ./dist/.",
        "build:ts": "npm run lint && del -f dist && tsc --sourceMap false",
        "start_serve": "npm i --production && node index.js",
        "test": "export ENV=local && jest",
        "test:watch": "npm run test -- --watch",
        "lint": "eslint . --ext .ts"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.4",
        "@types/node-fetch": "^2.6.4",
        "axios": "^1.5.0",
        "bcrypt": "^5.0.1",
        "chalk": "4.1.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "esm": "^3.2.25",
        "express": "^4.18.0",
        "express-graphql": "^0.12.0",
        "express-validator": "^6.14.0",
        "graphql": "^15.8.0",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.3",
        "mysql2": "^2.3.3",
        "node-fetch": "^3.3.2",
        "sequelize": "^6.19.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-validator": "^3.0.0",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/moment": "^2.13.0",
        "@types/mysql": "^2.15.21",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "cpy-cli": "^4.1.0",
        "del-cli": "^4.0.1",
        "eslint": "^8.18.0",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.3",
        "ts-jest": "^27.1.4",
